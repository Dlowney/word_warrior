<div class="level-container">
  <div class="container">
    <div class="row">
      <% @levels.each do |level| %>
        <div class="col-xs-12">
          <div class="level-row">
            <%= level.name %>
            <%= level.difficulty %>
            <%= link_to "", level_path(level), class: "level-row-link" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
