<div class="level-container">
  <div class="container text-center">
    <div class="row text-center">
      <% @levels.each do |level| %>
        <div class="col-xs-4">
          <div class="level-row">
            <%= level.name %>
            <%= link_to "", level_path(level), class: "level-row-link" %>
          </div>
        </div>
      <% end %>
    </div>
  </div>
</div>
